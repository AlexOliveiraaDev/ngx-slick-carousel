{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "src",
  "projects": {
    "ngx-slick-carousel": {
      "root": "src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "project": "src/ng-package.json",
            "tsConfig": "src/tsconfig.json"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}